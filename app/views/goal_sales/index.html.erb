<h1>Listing goal_sales</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Meta</th>
      <th>Reached</th>
      <th>Fechainicio</th>
      <th>Fechafin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @goal_sales.each do |goal_sale| %>
      <tr>
        <td><%= goal_sale.user_id %></td>
        <td><%= goal_sale.meta %></td>
        <td><%= goal_sale.reached %></td>
        <td><%= goal_sale.fechainicio %></td>
        <td><%= goal_sale.fechafin %></td>
        <td><%= link_to 'Show', goal_sale %></td>
        <td><%= link_to 'Edit', edit_goal_sale_path(goal_sale) %></td>
        <td><%= link_to 'Destroy', goal_sale, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Goal sale', new_goal_sale_path %>
